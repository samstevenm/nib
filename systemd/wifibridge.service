#/etc/system/systemd/wifibridge.service
[Unit]
Description=Wifi to Ethernet Route
After=network.target
Wants=network.target

[Service]
# Type=simple
Type=forking
WorkingDirectory=/opt
# Fix the paths below:
ExecStart=/bin/bash /opt/wifi-to-eth-bridge.sh start
Restart=on-failure
User=root

[Install]
WantedBy=multi-user.target